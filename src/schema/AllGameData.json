{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": true,
    "properties": {
        "activePlayer": {
            "properties": {
                "currentGold": {
                    "type": "number"
                }
            },
            "required": [
                "currentGold"
            ],
            "type": "object"
        },
        "allPlayers": {
            "items": {
                "additionalProperties": true,
                "properties": {
                    "championName": {
                        "type": "string"
                    }
                },
                "required": [
                    "championName"
                ],
                "type": "object"
            },
            "type": "array"
        }
    },
    "required": [
        "activePlayer",
        "allPlayers"
    ],
    "type": "object"
}

