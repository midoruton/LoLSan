{
  "identifier": "lolsan",
  "productName": "lolsan",
  "version": "0.1.0",
  "build": {
    "beforeDevCommand": "yarn.cmd dev",
    "beforeBuildCommand": "yarn.cmd build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "windows": [
      {
        "title": "lolsan",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}
